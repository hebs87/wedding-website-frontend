{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "grc/component/TemplateName.jsx",
        "story": "grc/component/TemplateName.stories.jsx",
        "style": "grc/component/TemplateName.styles.scss"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "grc/page/TemplateName.jsx",
        "story": "grc/page/TemplateName.stories.jsx",
        "style": "grc/page/TemplateName.styles.scss"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withLazy": false
    }
  }
}
